{
  "expo": {
    "name": "Signal",
    "slug": "Signal",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "A simple app that allows users to send a quick alert with a pre-set message and their location to trusted contacts if they feel unsafe.",
    "icon": "./assets/images/icon.png",
    "scheme": "signal",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d6580"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1d6580"
      },
      "package": "com.maxxdevs.signal",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ea41420-4069-4ca3-8673-6d07d19aced3"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2ea41420-4069-4ca3-8673-6d07d19aced3"
    }
  }
}
